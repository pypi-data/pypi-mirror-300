                },
                "stream": {
                  "Ref": "LogStreamCBF5916A"
                }
              }
            },
            "Name": "AppContainer"
          },
          {
            "Essential": true,
            "FirelensConfiguration": {
              "Type": "fluentbit"
            },
            "Image": "amazon/aws-for-fluent-bit:stable",
            "LogConfiguration": {
              "LogDriver": "awslogs",
              "Options": {
                "awslogs-group": {
                  "Ref": "FluentBitLogGroupC1683337"
                },
                "awslogs-stream-prefix": "FireLensTestLog",
                "awslogs-region": {
                  "Ref": "AWS::Region"
                },
                "mode": "non-blocking",
                "max-buffer-size": "26214400b"
              }
            },
            "Memory": 256,
            "Name": "FireLensContainer"
          }
        ],
        "Cpu": "256",
        "ExecutionRoleArn": {
          "Fn::GetAtt": [
            "ExecutionRole605A040B",
            "Arn"
          ]
        },
        "Family": "EcsFirelensStackFluentBitTaskDef38BB095E",
        "Memory": "512",
        "NetworkMode": "awsvpc",
        "RequiresCompatibilities": [
          "FARGATE"
        ],
