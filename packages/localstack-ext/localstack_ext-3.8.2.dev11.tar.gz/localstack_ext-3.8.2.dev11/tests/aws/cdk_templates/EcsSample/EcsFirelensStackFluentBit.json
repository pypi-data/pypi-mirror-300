            "Command": [
              "bash",
              "-c",
              "while :; do echo \"test log\"; sleep 2; done"
            ],
            "Essential": true,
            "Image": "debian",
            "LogConfiguration": {
              "LogDriver": "awsfirelens",
              "Options": {
                "Name": "kinesis",
                "region": {
