{
    "title":"mywattsmon configuration",
    "loglevel":"info",
    "logtofile":"false",
    "port":"42001",
    "window":{
        "title":"Status",
        "size":"max",
        "interval":{"default":30,"20:00-22:00":60,"22:00-05:00":300},
        "nightmode":{"timeframe":"22:00-05:00","colors":"ref0"},
        "colors":{
            "window":{"bg":"gray7"},
            "values":{
                "ref0":{"bg":"black","fg":"gray7"},
                "ref1":{
                    "bg+":"darkseagreen","fg+":"white",
                    "bg-":"#FFCFC9","fg-":"black"
                },
                "ref2":{"bg":"#FFCFC9","fg":"black"},
                "ref3":{"bg":"steelblue4","fg":"white"},
                "ref4":{"bg":"khaki","fg":"black"},
                "ref5":{"bg":"darkolivegreen1","fg":"black"},
                "ref6":{"bg":"gray7","fg":"white"}
            }
        },
        "grid":{
            "W":{
                "units":"M1","var":"power","colors":"ref1",
                "rownum":0,"colnum":0,"rowspan":2,"colspan":2
            },
            "kWh":{
                "units":"M1","var":"energy","colors":"ref2",
                "rownum":0,"colnum":2,"rowspan":2,"colspan":3
            },
            "L1":{
                "units":"M2,M3","var":"power","colors":"ref3",
                "rownum":2,"colnum":0,"rowspan":1,"colspan":1
            },
            "L2":{
                "units":"M4","var":"power","colors":"ref3",
                "rownum":2,"colnum":1,"rowspan":1,"colspan":1
            },
            "L3":{
                "units":"M5","var":"power","colors":"ref3",
                "rownum":2,"colnum":2,"rowspan":1,"colspan":1
            },
            "L4":{
                "units":"M6,M7,M8,M9","var":"power","colors":"ref3",
                "rownum":2,"colnum":3,"rowspan":1,"colspan":1
            },
            "CHARGE":{
                "units":"M10","var":"power","colors":"ref3",
                "rownum":2,"colnum":4,"rowspan":1,"colspan":1
            },
            "FEED1":{
                "units":"M11","var":"power","colors":"ref4",
                "rownum":3,"colnum":0,"rowspan":1,"colspan":1
            },
            "FEED2":{
                "units":"M12","var":"power","colors":"ref4",
                "rownum":3,"colnum":1,"rowspan":1,"colspan":1
            },
            "BATT1":{
                "units":"M13","var":"power","colors":"ref5",
                "rownum":3,"colnum":2,"rowspan":1,"colspan":1
            },
            "BATT2":{
                "units":"M14","var":"power","colors":"ref5",
                "rownum":3,"colnum":3,"rowspan":1,"colspan":1
            },
            "Refresh":{
                "units":"<refresh>","var":"<countdown>","colors":"ref6",
                "rownum":3,"colnum":4,"rowspan":1,"colspan":1
            }
        }
    },
    "database":{
        "times":["00:00","03:00","06:00","09:00","12:00","15:00",
                "18:00","21:00","23:59"],
        "tablename":"kwh",
        "columns":{
            "em":{"units":"M1","var":"energy"},
            "l1":{"units":"M2,M3","var":"energy"},
            "l2":{"units":"M4","var":"energy"},
            "l3":{"units":"M5","var":"energy"},
            "l4":{"units":"M6,M7,M8,M9","var":"energy"},
            "charge":{"units":"M10","var":"energy"},
            "feed1":{"units":"M11","var":"energy"},
            "feed2":{"units":"M12","var":"energy"},
            "batt1":{"units":"M13","var":"energy"},
            "batt2":{"units":"M14","var":"energy"}
        }
    },
    "devices":{
        "Mock":{
            "module":"mywattsmon.app.device.mock",
            "units":{
                "M1":{"uid":"m1"},
                "M2":{"uid":"m2"},
                "M3":{"uid":"m3"},
                "M4":{"uid":"m4"},
                "M5":{"uid":"m5"},
                "M6":{"uid":"m6"},
                "M7":{"uid":"m7"},
                "M8":{"uid":"m8"},
                "M9":{"uid":"m9"},
                "M10":{"uid":"m10"},
                "M11":{"uid":"m11"},
                "M12":{"uid":"m12"},
                "M13":{"uid":"m13"},
                "M14":{"uid":"m14"}
            }
        }
    }
}
